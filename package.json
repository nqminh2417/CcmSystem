{
  "name": "CcmSystem",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "android:clean": "cd android && gradlew.bat clean",
    "android:clean-debug": "yarn android:clean && yarn start:reset",
    "android:aab": "cd android && gradlew.bat bundleRelease",
    "android:apk": "cd android && gradlew.bat assembleRelease",
    "android:clean-aab": "cd android && gradlew.bat clean bundleRelease",
    "android:clean-apk": "cd android && gradlew.bat clean assembleRelease",
    "android:clean-dirs": "IF EXIST android\\build (rmdir /s /q android\\build) && IF EXIST android\\app\\build (rmdir /s /q android\\app\\build) && IF EXIST android\\app\\.cxx (rmdir /s /q android\\app\\.cxx)",
    "clean:deep": "IF EXIST node_modules (rmdir /s /q node_modules) && yarn cache clean && yarn android:clean-dirs"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native-vector-icons/fontawesome": "^12.4.0",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "@react-navigation/stack": "^7.6.4",
    "nativewind": "^4.2.1",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-config": "^1.6.0",
    "react-native-device-info": "^15.0.1",
    "react-native-file-viewer": "^2.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-mmkv": "^4.0.1",
    "react-native-nitro-modules": "^0.31.7",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^4.2.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-sound": "^0.13.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-worklets": "^0.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-test-renderer": "19.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22"
}